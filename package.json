    "vfile-reporter": "^6.0.1",
    "unist-builder": "^2.0.3",
    "unified": "^9.1.0",
    "typescript": "^3.9.7",
    "tinyify": "^3.0.0",
    "rehype-format": "^3.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-sanitize": "^3.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^8.0.3",
    "remark-rehype": "^7.0.0",
    "hast-util-sanitize": "^3.0.0",
    "hast": "^1.0.0",
    "dtslint": "^3.6.14"
